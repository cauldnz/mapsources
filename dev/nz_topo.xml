<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<customMultiLayerMapSource>
    <name>NZ Topo</name>
    <layers>
        <customMapSource>
            <name>Custom OSM Mapnik</name>
            <minZoom>0</minZoom>
            <maxZoom>5</maxZoom>
            <tileType>png</tileType>
            <tileUpdate>None</tileUpdate>
            <url>http://tile.openstreetmap.org/{$z}/{$x}/{$y}.png</url>
        </customMapSource>
        <customMapSource>
            <name>topo 250</name>
            <minZoom>5</minZoom>
            <maxZoom>15</maxZoom>
            <url>http://nz1.nztopomaps.com/{$z}/{$x}/{$y}.png</url>
        </customMapSource>
    </layers>
</customMultiLayerMapSource>
